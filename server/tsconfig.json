{
	"compilerOptions": {
	  "target": "esnext",
	  "module": "commonjs",
	  "esModuleInterop": true,
	  "strict": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "baseUrl": ".",
	  "paths": {
		"*": ["node_modules/*"]
	  },
	  "outDir": "./dist",
	  "rootDir": "./src"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
  }
  